/**
 * Computes the sum of two bits.
 */

CHIP HALFADDER{
    IN a, b;    
    OUT sum,carry;  

    PARTS:
    // Put you code here:
    NandXor(a=a, b=b, out=sum);
    NandAnd(a=a, b=b, out=carry);
}
